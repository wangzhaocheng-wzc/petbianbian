{
  "name": "pet-health-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:analyze": "tsc && vite build --mode analyze",
    "preview": "vite preview",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint:fix": "eslint src/**/*.{ts,tsx} --fix",
    "format": "prettier --write src/**/*.{ts,tsx}",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:ci": "playwright test --reporter=html,json",
    "test:e2e:performance": "node e2e/run-load-stress-tests.cjs && node e2e/run-interaction-performance-tests.cjs",
    "test:e2e:visual": "node e2e/run-visual-tests.cjs",
    "test:e2e:smoke": "playwright test --grep=\"@smoke\"",
    "test:e2e:regression": "playwright test --grep=\"@regression\"",
    "test:pet-permissions": "node e2e/run-pet-permissions-tests.cjs",
    "test:pet-permissions:access": "node e2e/run-pet-permissions-tests.cjs suite access",
    "test:pet-permissions:sharing": "node e2e/run-pet-permissions-tests.cjs suite sharing",
    "test:pet-permissions:privacy": "node e2e/run-pet-permissions-tests.cjs suite privacy",
    "test:pet-permissions:security": "node e2e/run-pet-permissions-tests.cjs suite security",
    "test:pet-permissions:performance": "node e2e/run-pet-permissions-tests.cjs performance",
    "cleanup:test-data": "node e2e/utils/cleanup-test-data.cjs",
    "test:analysis": "node e2e/run-analysis-tests.cjs",
    "test:analysis:upload": "node e2e/run-analysis-tests.cjs suite upload",
    "test:analysis:process": "node e2e/run-analysis-tests.cjs suite process",
    "test:analysis:results": "node e2e/run-analysis-tests.cjs suite results",
    "test:analysis:performance": "node e2e/run-analysis-tests.cjs performance",
    "cleanup:test-images": "node e2e/utils/cleanup-test-images.cjs",
    "test:integration": "node e2e/run-integration-tests.cjs",
    "test:integration:dataflow": "playwright test specs/integration/end-to-end-dataflow.spec.ts",
    "test:integration:api": "playwright test specs/integration/api-integration.spec.ts",
    "test:integration:database": "playwright test specs/integration/database-integration.spec.ts",
    "test:visual": "node e2e/run-visual-tests.cjs",
    "test:visual:full-page": "node e2e/run-visual-tests.cjs full-page",
    "test:visual:components": "node e2e/run-visual-tests.cjs components",
    "test:visual:themes": "node e2e/run-visual-tests.cjs themes",
    "test:visual:animations": "node e2e/run-visual-tests.cjs animations",
    "test:visual:cross-platform": "node e2e/run-visual-tests.cjs cross-platform",
    "test:visual:update": "node e2e/run-visual-tests.cjs --update-baselines",
    "baseline": "node e2e/baseline-cli.cjs",
    "baseline:list": "node e2e/baseline-cli.cjs list",
    "baseline:update": "node e2e/baseline-cli.cjs update",
    "baseline:backup": "node e2e/baseline-cli.cjs backup",
    "baseline:cleanup": "node e2e/baseline-cli.cjs cleanup",
    "baseline:report": "node e2e/baseline-cli.cjs report",
    "env:setup": "node e2e/scripts/setup-test-env.js setup",
    "env:cleanup": "node e2e/scripts/setup-test-env.js cleanup",
    "env:health": "node e2e/scripts/health-monitor.js check",
    "env:monitor": "node e2e/scripts/health-monitor.js monitor",
    "data:init": "node e2e/scripts/test-data-automation.js init",
    "data:clear": "node e2e/scripts/test-data-automation.js clear",
    "data:validate": "node e2e/scripts/test-data-automation.js validate",
    "results:collect": "node e2e/utils/test-results-collector.js",
    "results:aggregate": "node e2e/utils/aggregate-test-results.js",
    "quality:analyze": "node e2e/utils/test-quality-monitor.js analyze",
    "quality:report": "node e2e/utils/test-quality-monitor.js report",
    "ci:run": "node e2e/scripts/ci-integration.js",
    "ci:smoke": "node e2e/scripts/ci-integration.js --suite smoke",
    "ci:regression": "node e2e/scripts/ci-integration.js --suite regression",
    "test:stability:improve": "node e2e/scripts/run-stability-improvements.ts",
    "test:stability:analyze": "node e2e/scripts/improve-test-stability.ts --analyze-history --recommendations",
    "test:stability:recommendations": "node e2e/scripts/improve-test-stability.ts --recommendations",
    "test:stability:auto-fix": "node e2e/scripts/improve-test-stability.ts --auto-fix --enable-auto-fix",
    "test:stability:monitor": "node e2e/utils/test-stability-monitor.ts",
    "test:stability:report": "node e2e/scripts/generate-comprehensive-reports.ts --include-stability",
    "test:maintainability": "node e2e/run-maintainability-enhancement.cjs",
    "test:maintainability:analyze": "node e2e/run-maintainability-enhancement.cjs --analyze",
    "test:maintainability:refactor": "node e2e/run-maintainability-enhancement.cjs --refactor",
    "test:maintainability:quality": "node e2e/run-maintainability-enhancement.cjs --quality",
    "test:maintainability:docs": "node e2e/run-maintainability-enhancement.cjs --docs",
    "test:maintainability:quick": "node e2e/run-maintainability-enhancement.cjs --quick",
    "test:maintainability:apply": "node e2e/run-maintainability-enhancement.cjs --apply"
  },
  "dependencies": {
    "@chakra-ui/form-control": "^2.2.0",
    "@chakra-ui/icons": "^2.2.4",
    "@chakra-ui/react": "^3.27.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "axios": "^1.12.2",
    "chart.js": "^4.5.0",
    "date-fns": "^4.1.0",
    "form-data": "^4.0.4",
    "framer-motion": "^12.23.22",
    "lucide-react": "^0.321.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.43.5",
    "react-router-dom": "^6.8.1",
    "recharts": "^3.1.2",
    "workbox-window": "^7.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.0.0",
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.37.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jsdom": "^21.1.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.7",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.3",
    "vite": "^4.2.0",
    "vite-plugin-pwa": "^1.0.3",
    "vitest": "^0.25.8",
    "workbox-window": "^7.3.0"
  }
}
